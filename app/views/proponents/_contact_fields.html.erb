<div class="nested-fields">
  <h3> <%= Contact.model_name.human %> </h3>

  <div class="field">
    <%= f.input :phone %>
  </div>

  <div class="field">
    <%= f.input :phone_type, collection: @phone_type_enum, input_html: { class: "form-control" } %>
  </div>

  <%= link_to_remove_association "Remover contato", f %>
</div>